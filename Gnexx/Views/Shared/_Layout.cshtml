<!DOCTYPE html>
<html lang="en">
<head>
    <meta charset="utf-8" />
    <meta name="viewport" content="width=device-width, initial-scale=1.0" />
    <title>@ViewData["Title"] - Gnexx</title>

    <link rel="stylesheet" href="~/lib/bootstrap/dist/css/bootstrap.min.css" />
    <link rel="stylesheet" href="~/css/site.css" asp-append-version="true" />
    <link rel="stylesheet" href="~/css/News/news-styles.css" asp-append-version="true" />
    <link rel="stylesheet" href="~/css/Auth/register-styles.css" asp-append-version="true" />
    <link rel="stylesheet" href="~/css/Auth/login-styles.css" asp-append-version="true" />

    <link rel="stylesheet" href="~/Gnexx.styles.css" asp-append-version="true" />

    <link rel="preconnect" href="https://fonts.googleapis.com">
    <link rel="preconnect" href="https://fonts.gstatic.com" crossorigin>
    <link href="https://fonts.googleapis.com/css2?family=Archivo:wght@100;200;300;400;500;600;700;800;900&family=Inter:wght@100;200;300;400;500;600;700;800;900&display=swap" rel="stylesheet">
</head>
<body>
     <header>
        <nav class="navbar navbar-expand-sm navbar-toggleable-sm navbar-light border-bottom box-shadow mb-3">
                <div class="container-fluid">
                    <a class="navbar-brand"  asp-controller="Auth" asp-action="SignOut">Salir</a>
                    <a class="navbar-brand" asp-area="" asp-controller="Home" asp-action="Index">Gnexx</a>
                    <button class="navbar-toggler" type="button" data-bs-toggle="collapse" data-bs-target=".navbar-collapse" aria-controls="navbarSupportedContent"
                        aria-expanded="false" aria-label="Toggle navigation">
                        <span class="navbar-toggler-icon"></span>
                    </button>
                    <div class="navbar-collapse collapse d-sm-inline-flex justify-content-between">
                        <ul class="navbar-nav flex-grow-1">
                            <li class="nav-item">
                            <a class="nav-link text-white" asp-area="" asp-controller="Home" asp-action="Index">Home</a>
                            </li>
                            <li class="nav-item">
                                <a class="nav-link text-white" asp-area="" asp-controller="News" asp-action="Index">Noticias</a>
                            </li>
                            <li class="nav-item">
                            <a class="nav-link text-white" asp-area="" asp-controller="Community" asp-action="Index">Comunidad</a>
                            </li>
                            <li class="nav-item">
                            <a class="nav-link text-white" asp-area="" asp-controller="Team" asp-action="Index">Equipos</a>
                            </li>
                            <li class="nav-item">
                                <a class="nav-link text-white" asp-area="" asp-controller="Settings" asp-action="Index">Configuración</a>
                            </li>
                        </ul>
                    </div>
                </div>
            </nav>
        </header>

    <div>
        <main role="main" class="pb-3">
            @RenderBody()
        </main>
    </div>

        <footer class="border-top footer text-muted">
            <div class="container">
                &copy; 2023 - Gnexx
            </div>
        </footer>
 



    <script src="~/lib/jquery/dist/jquery.min.js"></script>
    <script src="~/lib/bootstrap/dist/js/bootstrap.bundle.min.js"></script>
    <script src="~/js/site.js" asp-append-version="true"></script>

    <script src="~/js/Auth/script_register.js" asp-append-version="true"></script>

    @await RenderSectionAsync("Scripts", required: false)
</body>
</html>
